prog: td.o raster.o renderer.o model.o animator.o clear_qk.o sound.o
	cc68x -g td.o clear_qk.o raster.o renderer.o model.o animator.o sound.o -o prog
	
test: td.o raster.o renderer.o model.o animator.o clear_qk.o sound.o
	cc68x -g td.o clear_qk.o raster.o renderer.o model.o animator.o sound.o -o test
	
soundtest: sound.o Tsound.o
	cc68x -g Tsound.o sound.o -o soundtest

td.o: td.c raster.o renderer.o model.o animator.o
	cc68x -g -c td.c
	
Tsound.o: Tsound.c sound.o
	cc68x -g -c Tsound.c
	
clear_qk.o: clear_qk.s
		gen -D -L2 clear_qk.s
 
animator.o: animator.c animator.h model.h raster.h td.h
	cc68x -g -c animator.c

raster.o: raster.c raster.h
	cc68x -g -c raster.c		

model.o: model.c model.h
	cc68x -g -c model.c

renderer.o: renderer.c renderer.h model.h raster.h
	cc68x -g -c renderer.c
	
sound.o: sound.c sound.h
	cc68x -g -c sound.c

